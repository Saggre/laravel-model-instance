{
  "name": "saggre/laravel-model-instanec",
  "description": "",
  "keywords": [
    "laravel",
    "model",
    "instance",
    "command",
    "creation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sakri Koskimies",
      "email": "sakri.koskimies@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/console": "^9.21|^10.0",
    "illuminate/contracts": "^9.0|^10.0",
    "illuminate/support": "^9.0|^10.0",
    "spatie/laravel-model-info": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "Saggre\\LaravelModelInstance\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Saggre\\LaravelModelInstance\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Saggre\\LaravelModelInstance\\ModelInstanceServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
