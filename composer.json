{
  "name": "saggre/laravel-model-instance",
  "description": "Create new instances of Eloquent models with CLI",
  "keywords": [
    "laravel",
    "model",
    "instance",
    "command",
    "creation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sakri Koskimies",
      "email": "sakri.koskimies@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/console": "*",
    "illuminate/contracts": "*",
    "illuminate/support": "*",
    "spatie/laravel-model-info": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12",
    "orchestra/testbench": "^10.9"
  },
  "autoload": {
    "psr-4": {
      "Saggre\\LaravelModelInstance\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Saggre\\LaravelModelInstance\\Tests\\": "tests/",
      "App\\": "workbench/app/",
      "Database\\Factories\\": "workbench/database/factories/",
      "Database\\Seeders\\": "workbench/database/seeders/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Saggre\\LaravelModelInstance\\ModelInstanceServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
    ]
  }
}